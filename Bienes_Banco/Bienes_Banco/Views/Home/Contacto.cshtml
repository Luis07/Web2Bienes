
@{
    ViewBag.Title = "Contacto";
}
<div>
    <div class="row center">
        <div class="col s12">
            <br>
            <div class="row">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1004847.3503540659!2d-84.4356706!3d10.327078100000016!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8fa065f26425a581%3A0xe36bb83779dd2b95!2sCd+Quesada!5e0!3m2!1ses-419!2scr!4v1465246005780" width="1200" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
            </div>
        </div>
        <div class="container">
            <div class="col s6">
                <div class="row">
                    <form class="col s12">
                        <div class="row">
                            <div class="input-field col s6">
                                <input placeholder="" id="first_name" type="text" class="validate">
                                <label for="first_name">First Name</label>
                            </div>
                            <div class="input-field col s6">
                                <input id="last_name" type="text" class="validate">
                                <label for="last_name">Last Name</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <input id="email" type="email" class="validate">
                                <label for="email">Email</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <label id="asunto" type="email" class="validate"></label>
                                <label for="asunto">Asunto</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s12">
                                <input id="contenido" type="text" class="validate">
                                <textarea class="medium" for="contenido" placeholder="Mensaje"></textarea>
                            </div>
                        </div>
                        <a class="waves-effect waves-light btn blue-grey darken-1">Enviar</a>
                    </form>
                </div>
            </div>
            <div class="col s6">
                <div class="row">
                    <div class="col s12 m10 right">
                        <div class="card blue-grey darken-1">
                            <div class="card-content white-text">
                                <span class="card-title">Bienes Raices</span>
                                <p>Somos una empresa sobre bienes rainces...</p>
                                <ul>
                                    <li>Email: bienesraices@gmail.com</li>
                                    <li>Telefono: 8888-8888</li>
                                </ul>
                            </div>
                            <div class="card-action">
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>